{
    "parser": "babel-eslint",
    "plugins": [
      "react",
      "react-native",
      "prettier"
    ],
    "parserOptions": {
      "ecmaFeatures": {
          "jsx": true,
          "modules": true
      }
    },
    "extends" : [
      "eslint:recommended",
      "plugin:react/recommended",
      "airbnb",
      "plugin:prettier/recommended",
      "prettier/react"
    ],
    "rules" : {
      "arrow-body-style" : "warn"
    }
  }